{
  "name": "solution",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css": "node-sass src/scss/bundle.scss -o public/css",
    "css-w": "node-sass src/scss/bundle.scss -w -o public/css",
    "js": "browserify src/js/main.js -o public/js/bundle.js -t [ babelify --presets [ es2015 ] ]",
    "js-w": "watchify src/js/main.js -o public/js/bundle.js -dv -t [ babelify --presets [ es2015 ] ]",
    "dev": "shell-exec 'npm run css-w' 'npm run js-w' 'nodemon server.js'",
    "start": "npm run css && npm run js && node server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "awesomplete": "^1.1.1",
    "express": "^4.14.0",
    "virtual-dom": "^2.1.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "node-sass": "^3.13.0",
    "watchify": "^3.7.0"
  }
}
